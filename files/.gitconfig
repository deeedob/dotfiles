[user]
	email = dennis.ob@protonmail.com
	name = deeedob
[alias]
	ci = commit
	st = status
	sub = submodule update --remote --merge
	di = diff
	co = checkout
	ann = blame
[core]
	editor = vim
	autocrlf = input
	pager = less -FRSX
[credential]
	helper = cache --timeout=36000
#[url "git@github.com:"]
#    insteadOf = https://github.com/
[commit]
	template = qt6/.commit-template
[rerere]
	enabled = true
[rebase]
	stat = true
[color]
	ui = auto
