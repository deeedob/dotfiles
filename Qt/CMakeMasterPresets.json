{
  "version": 6,
  "configurePresets": [
    {
      "name": "debug",
      "description": "Qt Debug Development Build for Master Repo",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/debug",
      "cacheVariables": {
        "WARNINGS_ARE_ERRORS": "OFF",
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",

        "BUILD_SHARED_LIBS": "ON",
        "FEATURE_developer_build": "ON",
        "FEATURE_sanitize_address": "OFF",
        "FEATURE_sanitize_undefined": "OFF",
        "FEATURE_processenvironment": "ON",
        "DFEATURE_freetype": "ON",
        "DFEATURE_harfbuzz": "ON",
        "DFEATURE_system_freetype": "OFF",
        "DFEATURE_system_harfbuzz": "OFF",
        "DFEATURE_system_sqlite": "OFF",
        "DFEATURE_system_zlib": "OFF",

        "QT_NAMESPACE": "A",
        "QT_UNITY_BUILD": "OFF",
        "QT_UNITY_BUILD_BATCH_SIZE": "64",
        "QT_USE_CCACHE": "ON",

        "BUILD_qtwebengine": "OFF",
        "QT_ALLOW_DOWNLOAD": "ON",

        "QT_BUILD_TESTS": "OFF",
        "QT_BUILD_EXAMPLES": "OFF",
        "QT_BUILD_BENCHMARKS": "OFF",
        "QT_BUILD_MANUAL_TESTS": "OFF",
        "QT_BUILD_TESTS_BY_DEFAULT": "OFF",
        "QT_BUILD_EXAMPLES_BY_DEFAULT": "OFF",
        "QT_BUILD_EXAMPLES_AS_EXTERNAL": "OFF"
      },
      "environment": {
        "QDOC_SHOW_INTERNAL": "1"
      }
    }
  ]
}

