{
  "version": 6,
  "configurePresets": [
    {
      "name": "default",
      "description": "Qt Default Development Build",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/default",
      "cacheVariables": {
        "FEATURE_developer_build": "ON",
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++",
        "CMAKE_PREFIX_PATH": "/usr/lib/llvm14/lib/cmake",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "FEATURE_sanitize_address": "ON",
        "FEATURE_sanitize_undefined": "OFF",
        "WARNINGS_ARE_ERRORS": "ON",
        "FEATURE_cxx20": "ON",
        "QT_NAMESPACE": "B",
        "QT_UNITY_BUILD": "ON",
        "QT_UNITY_BUILD_BATCH_SIZE": "32",
        "FEATURE_processenvironment": "ON",
        "QT_HOST_PATH": "/home/wayn/Qt/precompiled/dev/6.6.0/gcc_64",
        "QT_NO_PACKAGE_VERSION_CHECK": "ON",
        "QT_BUILD_TESTS": "ON",
        "QT_BUILD_EXAMPLES": "ON",
        "QT_BUILD_TESTS_BY_DEFAULT": "OFF",
        "QT_BUILD_EXAMPLES_BY_DEFAULT": "OFF",
        "QT_BUILD_EXAMPLES_AS_EXTERNAL": "ON"
      },
      "environment": {
        "QDOC_SHOW_INTERNAL": "1"
      }
    }
  ]
}
